<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cah.myapplication.app" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" >
    </uses-sdk>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="cah.myapplication.app.MainMenu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cah.myapplication.app.PlayGameActivity"
            android:label="@string/playGame" >
        </activity>
        <activity
            android:name="cah.myapplication.app.HowToPlay"
            android:label="@string/title_activity_how_to_play" >
        </activity>
        <activity
            android:name="cah.myapplication.app.DirtyGame"
            android:label="@string/title_activity_dirty_game" >
        </activity>
        <activity
            android:name="cah.myapplication.app.CleanGame"
            android:label="@string/title_activity_clean_game" >
        </activity>
        <activity
            android:name="com.mycompany.myapp.CleanGameInterface"
            android:label="@string/title_activity_clean_game_interface" >
        </activity>
        <activity
            android:name="cah.myapplication.app.GameSetup"
            android:label="@string/title_activity_game_setup" >
        </activity>
        <activity
            android:name="cah.myapplication.app.WhiteCard"
            android:label="@string/title_activity_white_card" >
        </activity>
        <activity
            android:name="cah.myapplication.app.BlackCard"
            android:label="@string/title_activity_black_card" >
        </activity>
        <activity
            android:name="cah.myapplication.app.Player"
            android:label="@string/title_activity_player" >
        </activity>
        <activity
            android:name="cah.myapplication.app.Game"
            android:label="@string/title_activity_game" >
        </activity>
    </application>

</manifest>
